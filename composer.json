{
  "name": "jgrossi/corcel",
  "replace": {
    "juniorgrossi/corcel": "self.version"
  },
  "description": "Use WordPress backend with Laravel or any PHP framework",
  "homepage": "http://github.com/corcel/corcel",
  "license": "MIT",
  "authors": [
    {
      "name": "Junior Grossi",
      "email": "juniorgro@gmail.com",
      "homepage": "http://jgrossi.com"
    }
  ],
  "require": {
    "php": "^8.0.2",
    "illuminate/database": "^9.0",
    "illuminate/filesystem": "^9.0",
    "illuminate/support": "^9.0",
    "fakerphp/faker": "^1.19"
  },
  "require-dev": {
    "laravel/legacy-factories": "^1.0",
    "orchestra/testbench": "^7.0",
    "mockery/mockery": "^1.4.4",
    "dms/phpunit-arraysubset-asserts": "^0.3"
  },
  "autoload": {
    "psr-4": {
      "Corcel\\": "src/",
      "Corcel\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/thanks": false
    }
  }
}
